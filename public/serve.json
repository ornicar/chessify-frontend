{
  "headers": [
    {
      "source" : "**/*.@(jpg|jpeg|gif|png)",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "max-age=7200"
      }]
    },
    {
      "source" : "**/*.@(js|css)",
      "headers" : [{
        "key" : "Cache-Control",
        "value": "300"
      }]
    }, 
    {
      "source" : "404.html",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "max-age=300"
      }]
    }
  ]
}
